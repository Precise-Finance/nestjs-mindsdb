{
  "name": "@precise/nestjs-mindsdb",
  "version": "1.0.15",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-juint": "mocha --opts .mocharc --reporter mocha-junit-reporter",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "publish-package": "npm run-script build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Precise-Finance/nestjs-mindsdb.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@precise/mindsdb-js-sdk": "^2.0.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.21",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/uuid": "^8.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "3.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "@nestjs/config": "^1.2.1 || ^2.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!/node_modules/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "coverageReporters": [
      "cobertura",
      "html"
    ],
    "testEnvironment": "node"
  }
}
